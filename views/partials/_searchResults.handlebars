<div class="row">
  <div class="col-md-8">
    <h5 class="mt-0 mb-0">Search Results:</h5>
    {{#if dogResults }}
    <table class="table table-hover mt-3 mb-5">
      <thead>
        <tr>
          <th scope="col">ID</th>
          <th scope="col">Name
          </th>
          <th scope="col">Gender</th>
          <th scope="col">Breed</th>
          <th scope="col">Age</th>
          <th scope="col">Temperament</th>
          <th scope="col">Owner</th>

        </tr>
      </thead>
      <tbody>
        {{#each dogResults }}
        <tr>
          <th scope="row">{{ this.id }}</th>
          <td>{{ this.name }}</td>
          <td>{{ this.gender }}</td>
          <td>{{ this.breed }}</td>
          <td>{{ this.age }}</td>
          <td>{{ this.temperament }}</td>
          <td><a href="/api/search/{{ this.userId }}">See Profile</a>
          </td>
        </tr>
        {{/each}}
      </tbody>
    </table>
    {{/if}}
    {{#unless dogResults }}
    <h6 class="mt-2">🐶 No results yet 🐶</h3>
    {{/unless}}
  </div>
</div>